//Morse Code 
void setup() {
  pinMode(6, OUTPUT);
  }

void loop() {
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(120);
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(120);
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(120);
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(360); //END LETTER
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(120);
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(600);  // END WORD
 digitalWrite(6, HIGH);
 delay(360);
 digitalWrite(6, LOW);
 delay(360); // END LETTER
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(120);
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(120);
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(120);
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(360);  // END LETTER
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(360);  // ENDS LETTER
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(120);
 digitalWrite(6, HIGH);
 delay(360);
 digitalWrite(6, LOW);
 delay(360);  
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(360); // end letter
 digitalWrite(6, HIGH);
 delay(120);
 digitalWrite(6, LOW);
 delay(120);
 delay(600); 

}
